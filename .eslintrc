{
  "env": {
      "commonjs": true,
      "es6": true,
      "node": true,
      "jest/globals": true
  },
  "parserOptions": {
      "sourceType": "module"
  },
  "extends": [
      "standard",
      "eslint:recommended"
  ],
  "rules": {
      "import/prefer-default-export": 0,
      "no-console": 0
  },
  "plugins": [
      "node",
      "jest"
  ]
}